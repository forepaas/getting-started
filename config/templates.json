{
  "recharts": {
    "options": {
      "font": "Source Sans Pro",
      "series": [
        {
          "fill": "rgba(0, 204, 249, 0.5)",
          "stroke": "#0089C0"
        }
      ],
      "barCategoryGap": 10,
      "cartesianGrid": {
        "stroke": "#f5f5f5",
        "vertical": false
      },
      "legend": {
        "hide": true,
        "height": 10,
        "verticalAlign": "top",
        "align": "right",
        "iconType": "circle",
        "iconSize": 9
      },
      "tooltip": {
        "style": {
          "padding": "10px",
          "backgroundColor": "rgba(62, 69, 80, 0.85)",
          "color": "white",
          "borderRadius": 10,
          "boxShadow": "inset 0 -3px 0 0 rgba(0,0,0,0.13)",
          "textAlign": "center"
        },
        "hideName": true,
        "style-label": {
          "fontSize": 11,
          "fontWeight": 600
        },
        "style-content": {
          "display": "flex",
          "justifyContent": "center"
        },
        "style-name": {
          "fontSize": 13
        },
        "style-value": {
          "fontSize": 13,
          "fontWeight": 600
        }
      },
      "xAxis": {
        "height": 20,
        "axisLine": false,
        "interval": 0,
        "tickLine": false,
        "tickStyle": {
          "y": -8,
          "fontSize": 12
        }
      },
      "yAxis": {
        "tickLine": false,
        "axisLine": false,
        "tickStyle": {
          "x": -24
        }
      }
    }
  },
  "recharts.donut": {
    "options": {
      "colors": [
        "#00CCF9",
        "#B18BFF",
        "#9EDF10",
        "#FF6F90",
        "#FACD30",
        "#61DDA8",
        "#0089C0"
      ],
      "series": [
        {
          "interval": 0,
          "labelLine": false,
          "stroke": null,
          "fill": null,
          "innerRadius": 50,
          "outerRadius": 60
        }
      ]
    }
  },
  "recharts.bar": {
    "options": {
      "xAxis": {
        "tickStyle": {
          "fontSize": 10
        }
      }
    }
  },
  "recharts.bar-horizontal": {
    "options": {
      "barSize": 6,
      "yAxis": {
        "hide": true
      },
      "xAxis": {
        "tickStyle": {
          "y": 0
        }
      }
    }
  },
  "recharts.scatter": {
    "extends": "recharts",
    "options": {
      "xAxis": {
        "tickStyle": {
          "y": -10,
          "fontSize": 12
        },
        "labelStyle": {
          "fill": "#97A7B7",
          "fontSize": 14,
          "offset": -25
        }
      },
      "yAxis": {
        "labelStyle": {
          "fill": "#97A7B7",
          "fontSize": 14,
          "angle": -92,
          "position": "left",
          "offset": -2
        }
      }
    }
  },
  "recharts.line": {
    "extends": "recharts"
  },
  "jvectormap": {
    "options": {
      "backgroundColor": "#272c33",
      "markerStyle": {
        "initial": {
          "fill": "#3d4554",
          "fill-opacity": 1,
          "stroke": "none",
          "stroke-width": 0,
          "stroke-opacity": 0
        },
        "hover": {
          "fill": "#1975d2"
        }
      },
      "regionStyle": {
        "initial": {
          "fill": "#e7e7e7",
          "fill-opacity": 1,
          "stroke": "#3d4554",
          "stroke-width": 1,
          "stroke-opacity": 1
        },
        "hover": {
          "fill": "#269efb"
        }
      }
    }
  },
  "morris": {
    "options": {
      "backgroundColor": "transparent",
      "labelColor": "rgba(255, 255, 255, 0.9)",
      "barColors": [
        "#1f87e5",
        "#fbb22c",
        "#ef534f",
        "#7460ee",
        "#26C706",
        "#f79490",
        "#ecedf1"
      ],
      "lineColors": [
        "#1f87e5",
        "#fbb22c",
        "#ef534f",
        "#7460ee",
        "#26C706",
        "#f79490",
        "#ecedf1"
      ],
      "colors": [
        "#1f87e5",
        "#fbb22c",
        "#ef534f",
        "#7460ee",
        "#26C706",
        "#f79490",
        "#ecedf1"
      ]
    }
  },
  "echarts": {
    "options": {
      "legend": {
        "inactiveColor": "#3d4554",
        "textStyle": {
          "color": "#bbbbbb"
        }
      },
      "color": [
        "#1f87e5",
        "#fbb22c",
        "#ef534f",
        "#7460ee",
        "#26C706",
        "#f79490",
        "#ecedf1"
      ]
    }
  },
  "table": {
    "options": {
      "rowHeight": 50,
      "headerHeight": 48,
      "search": {
        "icon": "fa fa-search"
      }
    }
  },
  "chartjs": {
    "options": {
      "options": {
        "legend": {
          "labels": {
            "fontColor": "#bbbbbb"
          }
        }
      },
      "borderColor": ["transparent"],
      "colors": [
        "#1f87e5",
        "#fbb22c",
        "#ef534f",
        "#7460ee",
        "#26C706",
        "#f79490",
        "#ecedf1"
      ]
    }
  }
}