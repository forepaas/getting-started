{
  "selectbox": {
    "input": {
      "height": 26
    },
    "indicatorsContainer": {
      "height": 34
    },
    "dropdownIcon": {
      "fontSize": 12,
      "padding": 10
    },
    "control": {
      "fontSize": 12,
      "width": 250,
      "borderRadius": 5,
      "maxHeight": 36,
      "height": 36,
      "minHeight": 0,
      "padding": 0,
      "borderColor": "lightGrey",
      "boxShadow": "none!important"
    },
    "multiValue": {
      "backgroundColor": "#ff5400",
      "borderRadius": 5,
      "height": 26,
      "color": "white"
    },
    "multiValueLabel": {
      "lineHeight": "20px",
      "color": "white"
    },
    "optionsSelected": {
      "background": "lightGrey",
      "color": "#ffffff"
    },
    "optionsFocused": {
      "background": "white",
      "fontSize": 16,
      "fontWeight": 700
    },
    "menuList": {
      "padding": 0
    },
    "menu": {
      "margin": 0,
      "fontSize": 12
    }
  },
  "recharts": {
    "font": "Source Sans Pro",
    "series": [
      {
        "fill": "rgba(0, 204, 249, 0.5)",
        "stroke": "#0089C0"
      }
    ],
    "barCategoryGap": 10,
    "cartesianGrid": {
      "stroke": "#f5f5f5",
      "vertical": false
    },
    "legend": {
      "hide": true,
      "height": 10,
      "verticalAlign": "top",
      "align": "right",
      "iconType": "circle",
      "iconSize": 9
    },
    "tooltip": {
      "style": {
        "padding": "10px",
        "backgroundColor": "rgba(62, 69, 80, 0.85)",
        "color": "white",
        "borderRadius": 10,
        "boxShadow": "inset 0 -3px 0 0 rgba(0,0,0,0.13)",
        "textAlign": "center"
      },
      "hideName": true,
      "style-label": {
        "fontSize": 11,
        "fontWeight": 600
      },
      "style-content": {
        "display": "flex",
        "justifyContent": "center"
      },
      "style-name": {
        "fontSize": 13
      },
      "style-value": {
        "fontSize": 13,
        "fontWeight": 600
      }
    },
    "xAxis": {
      "height": 20,
      "axisLine": false,
      "interval": 0,
      "tickLine": false,
      "tickStyle": {
        "y": -8,
        "fontSize": 12
      }
    },
    "yAxis": {
      "tickLine": false,
      "axisLine": false,
      "tickStyle": {
        "x": -24
      }
    }
  },
  "recharts.pie": {
    "colors": [
      "#00CCF9",
      "#B18BFF",
      "#9EDF10",
      "#FF6F90",
      "#FACD30",
      "#61DDA8",
      "#0089C0"
    ],
    "series": [
      {
        "interval": 0,
        "labelLine": false,
        "stroke": null,
        "fill": null,
        "innerRadius": 50,
        "outerRadius": 60
      }
    ]
  },

  "recharts.bar": {
    "xAxis": {
      "tickStyle": {
        "fontSize": 10
      }
    }
  },
  "recharts.bar-horizontal": {
    "barSize": 6,
    "yAxis": {
      "hide": true
    },
    "xAxis": {
      "tickStyle": {
        "y": 0
      }
    }
  }
}
